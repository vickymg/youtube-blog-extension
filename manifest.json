{
  "manifest_version": 2,

  "name": "youtube-blog-extension",
  "description": "an extension to show the latest YouTube engineering blog posts",
  "version": "1.0",

  "browser_action": {
    "default_title": "YouTube latest blogs",
    "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["background.js"]
    },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com;object-src 'self'"
}
